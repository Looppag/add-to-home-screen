    "type": "git",
    "url": "git+ssh://git@github.com:rajatsehgal/add-to-home-screen.git"
  },
  "keywords": [
    "mobile",
    "home screen",
    "web app"
  ],
  "author": "rajatsehgal <rajatsehgal1988@gmail.com> (rajatsehgal.github.io)",
  "license": "MIT",
  "homepage": "http://rajatsehgal.github.io/add-to-home-screen/",
  "dependencies": {
    "radium": "^0.18.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "http-server": "^0.9.0",
    "webpack": "^1.13.1"
  }
}
