{
  "name": "@rajatsehgal/add-to-home-screen",
  "version": "0.0.7",
  "description": "A React component that detects if the mobile web app is running in standalone mode or not and shows a banner at the bottom of the screen, prompting the user to add this app to the home screen.",
  "scripts": {
    "start": "http-server . -s",
    "build": "webpack -d",
    "watch": "webpack -d --watch",
    "prod": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:rajatsehgal/add-to-home-screen.git"
  },
  "keywords": [
    "mobile",
    "home screen",
    "web app"
  ],
  "author": "rajatsehgal <rajatsehgal1988@gmail.com> (rajatsehgal.github.io)",
  "license": "MIT",
  "homepage": "http://rajatsehgal.github.io/add-to-home-screen/",
  "dependencies": {
    "radium": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-1": "latest",
    "http-server": "latest",
    "webpack": "latest"
  }
}
