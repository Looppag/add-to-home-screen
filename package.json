  "homepage": "https://github.com/rajatsehgal/add-to-home-screen#readme",
  "dependencies": {
    "radium": "^0.16.6",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "http-server": "^0.9.0",
    "webpack": "^1.12.14"
  }
